   ],
   "source": [
    "def filter_list(l):\n",
    "    r = []\n",
    "    for i in l:\n",
    "        if type(i) == int:\n",
    "            r.append(i)\n",
    "    return r\n",
    "    'return a new list with the strings filtered out'\n",
    "filter_list([1,2,'a','b'])\n",
    "filter_list([1, \"a\", \"b\", 0, 15])\n",
    "filter_list([1,2,'a','b'])"
   ]
